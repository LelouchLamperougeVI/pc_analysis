[behavior,deconv]=convert_behavior(behavior,tcs,deconv);

analysis=pc_batch_analysis(behavior,deconv);

v2struct(analysis);
v2struct(behavior);

for i
seq=max(deconv(trials_ts(1):trials_ts(2),:));